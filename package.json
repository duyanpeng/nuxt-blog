{
  "name": "nuxt-blog",
  "version": "1.0.0",
  "description": "My groundbreaking Nuxt.js project",
  "author": "duyanpeng",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
